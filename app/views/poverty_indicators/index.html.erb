<div class="page-header">
  <h1><%= t :municipality_graph %></h1>
</div>

<div class="row">

  <div class="col-xs-3">
    <%= select_tag :formulas, options_for_select(formulas.map { |f| [t(f, scope: :formulas), f] }), class: 'form-control' %>
  </div>

  <div class="col-xs-2">
    <%= select_tag :abc, options_for_select(PovertyIndicator.indicators.map { |i| [t(i, scope: :indicators), i] }), class: 'form-control' %>
  </div>

  <div class="col-xs-4">
    <button type="submit" class="btn btn-default"><%= t :graphic %></button>
  </div>
</div>
